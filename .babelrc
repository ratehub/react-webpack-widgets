{
   "presets": [
      "react",
      "es2015",
      "es2017",
      "stage-1"
   ],
   "plugins": [
      "transform-decorators-legacy",
      "transform-runtime",
      "jsx-control-statements",
      "react-hot-loader/babel",
      [
         "react-intl",
         {
            "messagesDir": "./build/messages",
            "enforceDescriptions": false
         }
      ]
   ]
}
